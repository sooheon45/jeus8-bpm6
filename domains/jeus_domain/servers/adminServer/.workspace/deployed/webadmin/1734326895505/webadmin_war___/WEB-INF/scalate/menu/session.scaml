-@ val selected: Int = 0
-@ val disabled: Boolean = false

-@ val name: String = ""

- val url = Utils.buildUri("/session/specificscopecluster/{name}", name)
%ul.tab01
	-
		J.render("tab", Map(
			"items" -> List(
				TabItem(url, "message:menu.basic"),
				TabItem(url + "/sessionclusterconfig", "message:menu.clusters.sessionclusterconfig")
			),
			"selected" -> selected
		))
